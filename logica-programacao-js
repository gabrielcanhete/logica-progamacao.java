let numero = 10;
 
console.log(`\nO numero é inicialmente igual a: ${numero}\n`);

console.log(`Após atribuir somando o número 10 : ${numero +=10}`);

console.log(`Reatribuindo e subtraindo 10 : ${numero -=10}`);

console.log(`Reatribuindo multiplicando por 10 : ${numero *=10}`);

console.log(`Reatribuindo e dividindo por 10 : ${numero /=10}`);

console.log(`\x1b[36mReatribuindo e pegando o resto da divisão por 10 : ${numero %=10}`);



 